{
    "compilerOptions": {
        "target": "ES2022",
        "moduleResolution": "node",
        "noLib": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "sourceMap": false,
        "pretty": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "allowUnreachableCode": true,
        "allowUnusedLabels": true,
        "noImplicitAny": true,
        "noImplicitReturns": false,
        "noImplicitUseStrict": false,
        "rootDir": "./src",
        "outDir": "BP/scripts",
        "listFiles": false,
        "stripInternal": true,
        "noEmitHelpers": true
    },
    "exclude": [
        "node_modules",
    ],
    "compileOnSave": false
}